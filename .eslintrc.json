{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-refresh"
    ],
    "rules": {
        "no-mixed-spaces-and-tabs": "off",
        "react/prop-types": "off",
        "react-refresh/only-export-components": "warn"
    }
}